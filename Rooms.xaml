<Page x:Class="FrontDeskHotel.Rooms"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FrontDeskHotel"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Rooms"
      Loaded="Room_OnLoaded">

    <Grid>
        <Label Content="Rooms " HorizontalAlignment="Left" Margin="11,10,0,0" VerticalAlignment="Top" Height="27" Width="65" Background="White" FontFamily="Arial Black" RenderTransformOrigin="0.5,0.5" Grid.RowSpan="2"/>
        <Canvas x:Name="myCanvas" Margin="81,10,0,0" Grid.RowSpan="3">
            <Frame x:Name="roomsFrame" NavigationUIVisibility="Hidden" Margin="81,10,10,10"/>
        </Canvas>

    </Grid>
</Page>
